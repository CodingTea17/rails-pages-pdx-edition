<%= render('layouts/errors', :object => @business) %>

<%= form_for [@type, @business] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :telenum %>
  <%= f.text_field :telenum %><br>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :zip %>
  <%= f.text_field :zip %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :website %>
  <%= f.text_field :website %><br>
  <p><%= f.submit %></p>
<% end %>
